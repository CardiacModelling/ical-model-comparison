  <component name="calcium_current">
    <variable name="E_Ca" units="mV" initial_value="70.0"/>
    <variable name="ICa" units="uA_per_cm2" public_interface="out" cmeta:id="ICa"/>
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="d" units="dimensionless" public_interface="in"/>
    <variable name="driving_term_ohmic" units="mV" cmeta:id="driving_term_ohmic"/>
    <variable name="f" units="dimensionless" public_interface="in"/>
    <variable name="g_Ca" units="mS_per_cm2" initial_value="0.25"/>
    <variable name="open_prob" units="dimensionless" cmeta:id="open_prob"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>ICa</ci>
        <apply>
          <times/>
          <apply>
            <times/>
            <ci>g_Ca</ci>
            <ci>open_prob</ci>
          </apply>
          <ci>driving_term_ohmic</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>driving_term_ohmic</ci>
        <apply>
          <minus/>
          <ci>V</ci>
          <ci>E_Ca</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>open_prob</ci>
        <apply>
          <times/>
          <ci>d</ci>
          <ci>f</ci>
        </apply>
      </apply>
    </math>
  </component>
  <component name="calcium_current_d_gate">
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="d" units="dimensionless" public_interface="out" initial_value="2.93135000000000010e-03"/>
    <variable name="d_inf" units="dimensionless"/>
    <variable name="tau_d" units="ms"/>
    <variable name="time" units="ms" public_interface="in"/>
    <variable name="x" units="dimensionless"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>d</ci>
        </apply>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <ci>d_inf</ci>
            <ci>d</ci>
          </apply>
          <ci>tau_d</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>d_inf</ci>
        <apply>
          <divide/>
          <cn cellml:units="dimensionless">1.0</cn>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless">1.0</cn>
            <apply>
              <exp/>
              <ci>x</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>tau_d</ci>
        <apply>
          <divide/>
          <cn cellml:units="ms">60.0</cn>
          <apply>
            <plus/>
            <apply>
              <exp/>
              <apply>
                <times/>
                <apply>
                  <minus/>
                  <cn cellml:units="dimensionless">4.0</cn>
                </apply>
                <ci>x</ci>
              </apply>
            </apply>
            <apply>
              <exp/>
              <apply>
                <times/>
                <cn cellml:units="dimensionless">2.0</cn>
                <ci>x</ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>x</ci>
        <apply>
          <times/>
          <cn cellml:units="per_mV">0.008</cn>
          <apply>
            <plus/>
            <ci>V</ci>
            <cn cellml:units="mV">14.0</cn>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <component name="calcium_current_f_gate">
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="f" units="dimensionless" public_interface="out" initial_value="8.08739169999999952e-01"/>
    <variable name="f_inf" units="dimensionless"/>
    <variable name="tau_f" units="ms"/>
    <variable name="time" units="ms" public_interface="in"/>
    <variable name="y" units="dimensionless"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>f</ci>
        </apply>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <ci>f_inf</ci>
            <ci>f</ci>
          </apply>
          <ci>tau_f</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>f_inf</ci>
        <apply>
          <divide/>
          <cn cellml:units="dimensionless">1.0</cn>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless">1.0</cn>
            <apply>
              <exp/>
              <apply>
                <minus/>
                <ci>y</ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>tau_f</ci>
        <apply>
          <times/>
          <cn cellml:units="ms">170.0</cn>
          <apply>
            <exp/>
            <apply>
              <divide/>
              <ci>y</ci>
              <cn cellml:units="dimensionless">6.0</cn>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>y</ci>
        <apply>
          <times/>
          <cn cellml:units="per_mV">0.12</cn>
          <apply>
            <plus/>
            <ci>V</ci>
            <cn cellml:units="mV">27.0</cn>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <component name="environment">
    <variable name="time" units="ms" public_interface="out" cmeta:id="time"/>
  </component>
  <component name="membrane">
    <variable name="ICa" units="uA_per_cm2" public_interface="in"/>
    <variable name="V" units="mV" public_interface="out" initial_value="-7.80413669999999939e+01" cmeta:id="V"/>
    <variable name="time" units="ms" public_interface="in"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
