<?xml version='1.0' encoding='UTF-8'?>
<model xmlns="http://www.cellml.org/cellml/1.0#" xmlns:cellml="http://www.cellml.org/cellml/1.0#" xmlns:cmeta="http://www.cellml.org/metadata/1.0#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" name="bassingthwaighte_1972">
  <units name="cm2_per_uF">
    <unit units="gram" multiplier="100000"/>
    <unit units="metre" exponent="4.0"/>
    <unit units="second" exponent="-4.0"/>
    <unit units="ampere" exponent="-2.0"/>
  </units>
  <units name="mS_per_cm2">
    <unit units="gram" exponent="-1.0" multiplier="0.01"/>
    <unit units="metre" exponent="-4.0"/>
    <unit units="second" exponent="3.0"/>
    <unit units="ampere" exponent="2.0"/>
  </units>
  <units name="mV">
    <unit units="gram"/>
    <unit units="metre" exponent="2.0"/>
    <unit units="second" exponent="-3.0"/>
    <unit units="ampere" exponent="-1.0"/>
  </units>
  <units name="ms">
    <unit units="second" multiplier="0.001"/>
  </units>
  <units name="per_mV">
    <unit units="gram" exponent="-1.0"/>
    <unit units="metre" exponent="-2.0"/>
    <unit units="second" exponent="3.0"/>
    <unit units="ampere"/>
