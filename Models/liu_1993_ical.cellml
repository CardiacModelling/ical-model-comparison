  <component name="calcium_channel">
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="d" units="dimensionless" public_interface="in"/>
    <variable name="f" units="dimensionless" public_interface="in"/>
    <variable name="i_s" units="uA_per_cm2" cmeta:id="i_s"/>
    <variable name="open_prob" units="dimensionless" cmeta:id="calcium_channel_open_prob"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>i_s</ci>
        <apply>
          <times/>
          <apply>
            <times/>
            <apply>
              <times/>
              <cn cellml:units="uA_per_cm2">12.5</cn>
              <ci>d</ci>
            </apply>
            <ci>f</ci>
          </apply>
          <apply>
            <minus/>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
                  <minus/>
                  <ci>V</ci>
                  <cn cellml:units="mV">30.0</cn>
                </apply>
                <cn cellml:units="mV">15.0</cn>
              </apply>
            </apply>
            <cn cellml:units="dimensionless">1.0</cn>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>open_prob</ci>
        <apply>
          <times/>
          <ci>d</ci>
          <ci>f</ci>
        </apply>
      </apply>
    </math>
  </component>
  <component name="calcium_channel_d_gate">
    <variable name="V" units="mV" public_interface="in"/>
    <variable name="alpha_d" units="hertz"/>
    <variable name="beta_d" units="hertz"/>
    <variable name="d" units="dimensionless" public_interface="out" initial_value="3.2e-05"/>
    <variable name="time" units="second" public_interface="in"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>alpha_d</ci>
        <apply>
          <plus/>
          <apply>
            <divide/>
            <apply>
              <times/>
              <cn cellml:units="s2_A_per_g_per_m2">10.45</cn>
              <apply>
                <plus/>
                <ci>V</ci>
                <cn cellml:units="mV">35.0</cn>
              </apply>
            </apply>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless">1.0</cn>
              <apply>
                <exp/>
                <apply>
                  <divide/>
                  <apply>
                    <minus/>
                    <apply>
                      <plus/>
                      <ci>V</ci>
                      <cn cellml:units="mV">35.0</cn>
                    </apply>
                  </apply>
                  <cn cellml:units="mV">2.5</cn>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <divide/>
            <apply>
              <times/>
              <cn cellml:units="s2_A_per_g_per_m2">31.25</cn>
              <ci>V</ci>
            </apply>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless">1.0</cn>
              <apply>
                <exp/>
                <apply>
                  <divide/>
                  <apply>
                    <minus/>
                    <ci>V</ci>
                  </apply>
                  <cn cellml:units="mV">4.8</cn>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>beta_d</ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <minus/>
              <cn cellml:units="s2_A_per_g_per_m2">4.21</cn>
            </apply>
            <apply>
              <minus/>
              <ci>V</ci>
              <cn cellml:units="mV">5.0</cn>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <minus/>
              <apply>
                <exp/>
                <apply>
                  <divide/>
                  <apply>
                    <minus/>
                    <ci>V</ci>
                    <cn cellml:units="mV">5.0</cn>
                  </apply>
                  <cn cellml:units="mV">2.5</cn>
                </apply>
              </apply>
            </apply>
            <cn cellml:units="dimensionless">1.0</cn>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>d</ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci>alpha_d</ci>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless">1.0</cn>
              <ci>d</ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci>beta_d</ci>
            <ci>d</ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <component name="calcium_channel_f_gate">
    <variable name="V" units="mV" public_interface="in"/>
