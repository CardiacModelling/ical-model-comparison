    <variable name="duration" units="second" initial_value="0.5"/>
    <variable name="offset" units="second" initial_value="100.0"/>
    <variable name="period" units="second" initial_value="1000.0"/>
    <variable name="time" units="second" public_interface="in"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>V</ci>
        <piecewise>
          <piece>
            <cn cellml:units="mV">1.0</cn>
            <apply>
              <lt/>
              <apply>
                <minus/>
                <apply>
                  <minus/>
                  <ci>time</ci>
                  <ci>offset</ci>
                </apply>
                <apply>
                  <times/>
                  <ci>period</ci>
                  <apply>
                    <floor/>
                    <apply>
                      <divide/>
                      <apply>
                        <minus/>
                        <ci>time</ci>
                        <ci>offset</ci>
                      </apply>
                      <ci>period</ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
              <ci>duration</ci>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="mV">0.0</cn>
          </otherwise>
        </piecewise>
      </apply>
    </math>
  </component>
