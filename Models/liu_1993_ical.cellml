        <ci>beta_f</ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <cn cellml:units="s2_A_per_g_per_m2">0.944</cn>
            <apply>
              <plus/>
              <ci>V</ci>
              <cn cellml:units="mV">60.0</cn>
            </apply>
          </apply>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless">1.0</cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
                  <minus/>
                  <apply>
                    <plus/>
                    <ci>V</ci>
                    <cn cellml:units="mV">29.5</cn>
                  </apply>
                </apply>
                <cn cellml:units="mV">4.16</cn>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>f</ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci>alpha_f</ci>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless">1.0</cn>
              <ci>f</ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci>beta_f</ci>
