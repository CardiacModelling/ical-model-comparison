            <apply>
              <plus/>
              <ci>V</ci>
              <cn cellml:units="mV">20.0</cn>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <minus/>
              <apply>
                <exp/>
                <apply>
                  <divide/>
                  <apply>
                    <plus/>
                    <ci>V</ci>
                    <cn cellml:units="mV">20.0</cn>
                  </apply>
                  <cn cellml:units="mV">5.633</cn>
                </apply>
              </apply>
            </apply>
            <cn cellml:units="dimensionless">1.0</cn>
          </apply>
        </apply>
      </apply>
      <apply>
