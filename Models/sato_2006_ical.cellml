        <ci>JCa</ci>
        <apply>
          <times/>
          <apply>
            <times/>
            <apply>
              <minus/>
              <ci>gCa</ci>
            </apply>
            <ci>Open_prob</ci>
          </apply>
          <ci>driving_term_ghk</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>Open_prob</ci>
        <apply>
          <times/>
          <apply>
            <times/>
            <ci>f</ci>
            <ci>d</ci>
          </apply>
          <ci>f_Ca</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <ci>driving_term_ghk</ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <times/>
                  <cn cellml:units="dimensionless">4.0</cn>
                  <ci>V</ci>
                </apply>
                <apply>
                  <power/>
                  <ci>F</ci>
                  <cn cellml:units="dimensionless">2.0</cn>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci>R</ci>
                <ci>T</ci>
              </apply>
            </apply>
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci>Ca_s</ci>
                <apply>
                  <exp/>
                  <apply>
                    <divide/>
                    <apply>
                      <times/>
                      <apply>
                        <times/>
                        <cn cellml:units="dimensionless">2.0</cn>
                        <ci>V</ci>
                      </apply>
                      <ci>F</ci>
                    </apply>
                    <apply>
                      <times/>
                      <ci>R</ci>
                      <ci>T</ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
              <apply>
                <times/>
                <cn cellml:units="dimensionless">0.341</cn>
                <ci>Ca_o</ci>
              </apply>
            </apply>
          </apply>
          <apply>
            <minus/>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <apply>
                    <times/>
                    <cn cellml:units="dimensionless">2.0</cn>
                    <ci>V</ci>
                  </apply>
                  <ci>F</ci>
                </apply>
                <apply>
                  <times/>
                  <ci>R</ci>
                  <ci>T</ci>
                </apply>
              </apply>
            </apply>
            <cn cellml:units="dimensionless">1.0</cn>
          </apply>
        </apply>
      </apply>
